{
  "expo": {
    "name": "A.B.E.L.",
    "slug": "abel-assistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "abel",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.abel.assistant",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "A.B.E.L. utilise la caméra pour la fonctionnalité Vision IA",
        "NSMicrophoneUsageDescription": "A.B.E.L. utilise le microphone pour la reconnaissance vocale",
        "NSFaceIDUsageDescription": "A.B.E.L. utilise Face ID pour sécuriser votre session",
        "NSSpeechRecognitionUsageDescription": "A.B.E.L. utilise la reconnaissance vocale pour les commandes",
        "NSPhotoLibraryUsageDescription": "A.B.E.L. utilise votre galerie pour analyser des images avec Vision IA",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": []
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0f"
      },
      "package": "com.abel.assistant",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "blockedPermissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-local-authentication",
      [
        "expo-camera",
        {
          "cameraPermission": "A.B.E.L. a besoin d'accéder à votre caméra pour la Vision IA"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "A.B.E.L. a besoin d'accéder au microphone pour les commandes vocales"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "A.B.E.L. a besoin d'accéder à vos photos pour la Vision IA"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "abel-assistant-project-id"
      }
    },
    "owner": "adam-blf",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/abel-assistant-project-id"
    }
  }
}
