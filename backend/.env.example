# ============ AI/ML SERVICES ============
# Anthropic
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key

# Hugging Face
HUGGINGFACE_API_KEY=hf_your-huggingface-key

# OpenAI
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
